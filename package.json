{
  "name": "gguf-editor",
  "displayName": "gguf editor",
  "description": "A VS Code extension for viewing and inspecting GGUF model files",
  "version": "0.0.7",
  "publisher": "gguf",
  "author": {
    "name": "gguf",
    "email": "info@gguf.us"
  },
  "license": "MIT",
  "homepage": "https://github.com/mochiyaki/gguf-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/mochiyaki/gguf-editor"
  },
  "icon": "assets/images/icon.png",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gguf-editor.open",
        "title": "Open gguf-editor",
        "category": "gguf-editor"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "gguf-editor.open",
          "group": "gguf-editor",
          "when": "resourceExtname == .gguf"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "gguf editor",
      "properties": {
        "gguf-editor.maxArrayElements": {
          "type": "number",
          "default": 25,
          "scope": "window",
          "description": "Maximum number of elements to display in an array."
        }
      }
    }
  },
    "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@huggingface/gguf": "^0.3.2"
  }
}
